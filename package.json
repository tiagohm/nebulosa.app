		"compile": "bun build --compile --minify --production main.ts --outfile nebulosa.exe",
		"typecheck": "tsc --noEmit"
	},
	"author": {
		"name": "Tiago Melo",
		"email": "tiago.henrique.cco@gmail.com"
	},
	"license": "MIT",
	"dependencies": {
		"@elysiajs/cors": "1.3.3",
		"@elysiajs/cron": "1.3.0",
		"@heroui/react": "2.8.2",
		"@uidotdev/usehooks": "2.4.1",
		"@use-gesture/react": "10.3.1",
		"bun-plugin-tailwind": "0.0.15",
		"bunshi": "2.1.5",
		"date-fns": "4.1.0",
		"elysia": "1.3.8",
		"nebulosa": "github:tiagohm/nebulosa.ts",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"react-leaflet": "5.0.0",
		"tailwindcss": "4.1.12",
		"valtio": "2.1.5",
		"wretch": "2.11.0"
