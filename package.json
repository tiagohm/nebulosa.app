	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@types/bun": "1.2.20",
		"@types/leaflet": "1.9.20",
		"@types/react": "19.1.10",
		"@types/react-dom": "19.1.7",
		"typescript": "5.9.2"
	},
	"trustedDependencies": [
		"@biomejs/biome",
